@model Negocio.DTO.DTO_buscar_insumo_serie
@{
    ViewBag.Title = "Detalle";
}


<div class="container">

    <!-- Page-Title -->
    <div class="row">
        <div class="col-sm-12">

            <h4 class="page-title">Detalle del insumo</h4>
            <ol class="breadcrumb">
                <li>
                    <a href="~/Inicio/index">Inicio</a>
                </li>
                <li class="active">
                    Detalle del insumo
                </li>
            </ol>
        </div>
    </div>


    <div class="row">
        <div class="col-sm-10 col-sm-offset-1">
            <div class="card-box">
                <h4 class="m-t-0 header-title"><b>Detalle de la Computadora Seleccionada</b></h4>
                <p class="text-muted m-b-30 font-13">
                    Esta pantalla muestra la información detallada para el insumo ingresado.
                </p>

                <div class="row">
                    <div class="col-md-12">
                        <form class="form-horizontal" role="form">

                            <div class="form-group">
                                <label class="col-md-2 control-label">Numero de serie del insumo</label>
                                <div class="col-md-10">

                                    <label class="form-control" readonly="">@Model.nro_serie</label>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-md-2 control-label">Nombre del insumo</label>
                                <div class="col-md-10">

                                    <label class="form-control" readonly="">@Model.nombre_insumo</label>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-md-2 control-label">Computadora donde se encuentra</label>
                                <div class="col-md-10">

                                    <label class="form-control" readonly="">@Model.nombre_equipo</label>
                                </div>
                            </div>


                            <div class="form-group">
                                <label class="col-md-2 control-label">Número de ip de la computadora</label>
                                <div class="col-md-10">

                                    <label class="form-control" readonly="">@Model.ip_equipo</label>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-md-2 control-label">Ubicacion del insumo</label>
                                <div class="col-md-10">

                                    <label class="form-control" readonly="">@Model.ubicado_en</label>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-md-2 control-label">Fecha de colocación</label>
                                <div class="col-md-10">
                                    <label class="form-control" readonly="">@Model.fecha_colocacion</label>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-md-2 control-label">Observacion de la computadora</label>
                                <div class="col-md-10">
                                    <label class="form-control" readonly="">@Model.observacion_pc</label>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-2 control-label">Responsable</label>
                                <div class="col-md-10">
                                    <label class="form-control" readonly="">@Model.responsable</label>
                                </div>
                            </div>
                        </form>
                    </div>

                </div>

            </div>
        </div>

    </div>

</div>


